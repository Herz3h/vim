{
    "languageserver": {
        "intelephense": {
            "command": "node",
            "args": ["/usr/local/bin/intelephense", "--stdio"],
            "filetypes": ["php"]
        },
        "solargraph": {
            "command": "solargraph",
            "args": ["stdio"],
            "filetypes": ["ruby"]
        }
    },
    "vetur.dev.vlsPath": "/Users/jpldev/.config/yarn/global/node_modules/vls",
    "python":
    {
        "jediEnabled": false
    },
    "intelephense": {
        "licenceKey": "00YYJ8X6S3MHA3L",
